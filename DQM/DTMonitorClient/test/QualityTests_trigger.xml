	<PARAM name="warning">0.70</PARAM>
	<PARAM name="useEmptyBins">1</PARAM>
</QTEST>


<QTEST name="TrigPhiInterceptInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">-15</PARAM>
	<PARAM name="ymax">15</PARAM>
