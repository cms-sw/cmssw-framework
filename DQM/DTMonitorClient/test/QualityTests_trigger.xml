<TESTSCONFIGURATION>

<QTEST name="TrigCorrFracInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.50</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.80</PARAM>
	<PARAM name="warning">0.90</PARAM>
	<PARAM name="useEmptyBins">1</PARAM>
</QTEST>


<QTEST name="Trig2ndFracInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.0</PARAM>
	<PARAM name="ymax">0.25</PARAM>
	<PARAM name="error">0.90</PARAM>
	<PARAM name="warning">0.95</PARAM>
	<PARAM name="useEmptyBins">1</PARAM>
</QTEST>


<QTEST name="TrigPhiEfficiencyInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.5</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.95</PARAM>
	<PARAM name="warning">0.95</PARAM>
	<PARAM name="useEmptyBins">1</PARAM>
</QTEST>


<QTEST name="TrigPhiSlopeInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.92</PARAM>
	<PARAM name="ymax">1.08</PARAM>
	<PARAM name="error">0.80</PARAM>
	<PARAM name="warning">0.90</PARAM>
	<PARAM name="useEmptyBins">1</PARAM>
</QTEST>


<QTEST name="TrigPhibSlopeInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.92</PARAM>
	<PARAM name="ymax">1.08</PARAM>
