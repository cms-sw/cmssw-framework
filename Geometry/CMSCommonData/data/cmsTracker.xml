<?xml version="1.0"?>
<!-- 2006-09-19 Michael Case:   This file links the tracker:Tracker 
     LogicalPart to the cms:CMSE LogicalPart.  By leaving this file
     out of the configuration one can have Tracker as the root
     node in the graph. 
     -->
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../../DetectorDescription/Schema/DDLSchema.xsd">
 <PosPartSection label="cms.xml">
  <PosPart copyNumber="1">
   <rParent name="cms:CMSE"/>
   <rChild name="tracker:Tracker"/>
   <rRotation name="rotations:000D"/>
  </PosPart>
 </PosPartSection>
</DDDefinition>
