<TESTSCONFIGURATION>

<QTEST name="KolmogorovQTest0.3">
        <TYPE>Comp2RefKolmogorov</TYPE>	
	<PARAM name="testparam">0</PARAM>
	<PARAM name="error">0.3</PARAM>
	<PARAM name="warning">0.3</PARAM>
</QTEST>

<QTEST name="Chi2QTest0.01">
